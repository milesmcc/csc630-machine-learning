    "def collatz(starting_number):\n",
    "    if starting_number == 1:\n",
    "        return [1]\n",
    "    if starting_number % 2 == 0:\n",
    "        return [starting_number, *collatz(starting_number / 2)]\n",
    "    else:\n",
    "        return [starting_number, *collatz(starting_number * 3 + 1)]"
   ]
